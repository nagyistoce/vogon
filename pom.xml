				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>org.zlogic.vogon</groupId>
				<artifactId>vogon-web</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>com.h2database</groupId>
